<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="10-10"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="259.8dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft = "true"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textViewGrupo"
        android:layout_marginBottom="0.7dp"
        android:textStyle="bold"
        android:textColor="#FFFF"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="15.0dp"
        android:textSize="16sp"
        android:layout_marginTop="15dp"
        android:layout_alignParentTop="true" />
         />
    <TextView
        android:text="2019"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="259.4dp"
        android:layout_height="20dp"
        android:layout_below = "@id/textViewGrupo"
        android:layout_marginLeft="0.0dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/textViewAnio"
        android:layout_marginBottom="1.9dp"
        android:textStyle="bold"
        android:textColor="#FFFF"
        android:textSize="16sp"
        android:layout_marginTop="10dp"
        android:layout_alignLeft="@id/textViewGrupo" />
    <Button
        android:textColor="#3282b8"
        android:background="@drawable/button_border_edit"
        android:minWidth="25px"
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="37.9dp"
        android:layout_marginTop="14.4dp"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:id="@+id/btnOpcionesG"
        android:layout_marginLeft="22.1dp" />

   
</RelativeLayout>
      
